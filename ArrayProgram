import java.lang.*;
import java.util.Scanner;
class Dimensional
{
public static void main(String[]args)
{
int n=0;
int sum=0;
int arr[]=new int[10];
Scanner s=new Scanner(System.in);
System.out.println("enter number");
n=s.nextInt();
System.out.println("enter array values");
for (int i=0;i<n;i++)
{
arr[i]=s.nextInt();
}
for (int i=0;i<n;i++)
{
sum=sum+arr[i];

}
System.out.println(sum);
}
}
