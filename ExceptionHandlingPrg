public class pg1 {
int divnum(int m,int n) throws ArithmeticException
{
int div=m/n;

return div;
}
public static void main(String []args)	{
pg1 t= new pg1();
try {
System.out.println(t.divnum(45,0));
}
catch(ArithmeticException e)
{
System.out.println(e);
System.out.println("number cannot be divided");
}
}
}
