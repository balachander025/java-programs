public class Login {
	
	public static void main(String[] args) {
		int age=15;
		
		if(age>=18) {
			if(age==30) {
				System.out.println("Vote, age is equal");
			}
			else {
				System.out.println("Vote, age is not equal");
			}
		}
		else {
			System.out.println("Not eligible to vote");
		}
     		}   
	}
