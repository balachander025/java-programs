import java.lang.*;
import java.util.Scanner;
class BA
{
int a=10,b=20;
}
class BC extends BA
{
int a=10,b=20;
void add()
{
int a=10;
int b=20;
System.out.println(a+b);
System.out.println(this.a+this.b);
System.out.println(super.a+super.b);
}
public static void main(String[]args)
{
BC b=new BC();
b.add();
}
}
