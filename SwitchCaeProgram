public class Login {
	
	public static void main(String[] args) {
		int rollno=9;
		switch(rollno) {
		case 3:
			System.out.println("Bala");
			break;
		case 4:
			System.out.println("Hari");
			break;
		case 5:
			System.out.println("Gokul");
			break;
		case 6:
			System.out.println("Tamil");
			break;
			
		default:System.out.println("No name match");
		}
     		}   
	}
