import java.util.ArrayList;

public class datastructureprg1 {
    public static void main(String[] args) {
        ArrayList<String> list = new ArrayList<>();
        list.add("Apple");
        list.add("Banana");
        list.add("Orange");
        list.add("Banana");

        for (String item : list) {
            System.out.println(item);
        }
    }
}
