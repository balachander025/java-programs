import java.lang.*;
class Employer
{
int id=17;
}
class Program extends Employer
{
int exp=3;
}
class Projectdone extends Program
{
String a="Event management";
public static void main(String[]args)
{
Projectdone D=new Projectdone();
System.out.println(D.id);
System.out.println(D.exp);
System.out.println(D.a);
}
}
